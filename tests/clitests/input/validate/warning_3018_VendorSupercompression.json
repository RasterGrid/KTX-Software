{
    "description": "Vendor-specific supercompression scheme",
    "command": "ktx validate ${warnAsErr[flag]} --format ${format[name]} input/images/warning_3018_VendorSupercompression_${subcase}.ktx2",
    "status": "${warnAsErr[status]}",
    "stdout": "golden/validate/${warnAsErr[outPrefix]}_3018_VendorSupercompression_${subcase}.out.${format[ext]}",
    "args": {
        "warnAsErr": [
            {
                "flag": "",
                "status": 0,
                "outPrefix": "warning"
            },
            {
                "flag": "--warnings-as-errors",
                "status": 3,
                "outPrefix": "error"
            }
        ],
        "format": [
            {
                "name": "text",
                "ext": "txt"
            },
            {
                "name": "json",
                "ext": "json"
            },
            {
                "name": "mini-json",
                "ext": "mini.json"
            }
        ],
        "subcase": [
            "first",
            "01",
            "last"
        ]
    }
}
