{
    "description": "Warn on 3D Array texture",
    "command": "ktx validate ${warnAsErr[flag]} --format ${format[name]} input/images/warning_3014_ThreeDArray.ktx2",
    "status": "${warnAsErr[status]}",
    "stdout": "golden/validate/${warnAsErr[outPrefix]}_3014_ThreeDArray.out.${format[ext]}",
    "args": {
        "warnAsErr": [
            {
                "flag": "",
                "status": 0,
                "outPrefix": "warning"
            },
            {
                "flag": "--warnings-as-errors",
                "status": 3,
                "outPrefix": "error"
            }
        ],
        "format": [
            {
                "name": "text",
                "ext": "txt"
            },
            {
                "name": "json",
                "ext": "json"
            },
            {
                "name": "mini-json",
                "ext": "mini.json"
            }
        ]
    }
}
