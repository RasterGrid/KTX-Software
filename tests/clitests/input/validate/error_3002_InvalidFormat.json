{
    "description": "VkFormat outside of valid ranges",
    "command": "ktx validate --format ${format[name]} input/images/error_3002_InvalidFormat_${vkformat}.ktx2",
    "status": 3,
    "stdout": "golden/validate/error_3002_InvalidFormat_${vkformat}.out.${format[ext]}",
    "args": {
        "format": [
            {
                "name": "text",
                "ext": "txt"
            },
            {
                "name": "json",
                "ext": "json"
            },
            {
                "name": "mini-json",
                "ext": "mini.json"
            }
        ],
        "vkformat": [
            "199",
            "999999999",
            "0xFEEDFEED"
        ]
    }
}
